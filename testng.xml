<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="PetSuite">
    <listeners> <!-- This is needed to generate the Extent Report -->
        <listener class-name="api.utilities.ExtentReportingUtility"/>
    </listeners>
    <test thread-count="5" name="PetTests">
        <classes>
            <class name="api.tests.UserTests"/>
